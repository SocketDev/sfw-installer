{
  "name": "@socketsecurity/sfw-lite",
  "version": "1.1.0",
  "description": "Socket Firewall wrapper that verifies latest release and then runs npm with original args.",
  "license": "MIT",
  "bin": {
    "sfw": "src/dist/sfw.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/proper-lockfile": "^4.1.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "proper-lockfile": "4.1.2"
  },
  "bundledDependencies": [
    "proper-lockfile",
    "graceful-fs",
    "retry",
    "signal-exit"
  ],
  "//": "lock transitives so we can avoid vulnerabilities even if lockfile ignored",
  "overrides": {
    "proper-lockfile": {
      "graceful-fs": "4.2.11",
      "retry": "0.12.0",
      "signal-exit": "3.0.7"
    }
  },
  "preferGlobal": true
}
